{
    "name": "ejdelmonico/laravel-rss-feed",
    "type": "package",
    "description": "A simple Laravel 5 wrapper around SimplePie to pull in RSS feeds",
    "keywords": [
        "ejdelmonico",
        "Laravel",
        "rss",
        "SimplePie"
    ],
    "homepage": "https://github.com/ejdelmonico/LaravelRSSFeed",
    "license": "MIT",
    "authors": [
        {
            "name": "E J Del Monico",
            "email": "ejdelmonico@gmail.com",
            "homepage": "https://edwarddelmonico.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.3.0",
        "illuminate/contracts": "~5.3.0",
        "php": "^7.0",
        "simplepie/simplepie": "1.4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "orchestra/testbench": "~3.3.0"
    },
    "autoload": {
        "psr-4": {
            "ejdelmonico\\LaravelRSSFeed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ejdelmonico\\LaravelRSSFeed\\": "tests"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
